---
import { cn } from '@/lib/utils';
type Props = {
  class?: string;
};
const { class: ClassName } = Astro.props;
---

<button
  class={cn(
    'min-w-[152px] h-12 bg-black hover:bg-foreground transition-all text-white text-xl font-medium rounded-full tracking-widest px-5',
    ClassName
  )}
>
  <slot />
</button>
